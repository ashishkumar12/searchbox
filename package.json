    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack_config/dev/webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack_config/dev/webpack.client.js --watch",
    "prod": "npm-run-all --parallel prod:*",
    "prod-build": "npm-run-all --parallel prod:build-server prod:build-client",
    "prod:server": "BABEL_ENV=prod NODE_ENV=production node build/bundle.js",
    "prod:build-server": "BABEL_ENV=prod webpack --config webpack_config/production/webpack.server.js",
    "prod:build-client": "BABEL_ENV=prod webpack --config webpack_config/production/webpack.client.js"
